<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" style="-fx-background-color: #D0ECE7;" stylesheets="@../styles/controll_buttons.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.networkgui.mainPage.ProfileController">
   <children>
      <BorderPane prefHeight="720.0" prefWidth="1280.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <center>
            <VBox prefHeight="595.0" prefWidth="1092.0" style="-fx-background-color: #D0ECE7;" BorderPane.alignment="CENTER">
               <children>
                  <Label prefHeight="50.0" prefWidth="200.0" text="City:">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
                  <Label fx:id="city" prefHeight="50.0" prefWidth="400.0" text="user city">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
                  <Label prefHeight="50.0" prefWidth="200.0" text="Date of birth:">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
                  <Label fx:id="dateOfBirth" prefHeight="50.0" prefWidth="200.0" text="user date of birth ">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
               </children>
               <padding>
                  <Insets bottom="10.0" left="50.0" right="50.0" top="10.0" />
               </padding>
            </VBox>
         </center>
         <top>
            <VBox prefHeight="100.0" prefWidth="1280.0" style="-fx-background-color: #003b3b;" BorderPane.alignment="CENTER">
               <children>
                  <Label fx:id="name" alignment="CENTER" prefHeight="88.0" prefWidth="1280.0" text="Name" textAlignment="CENTER" textFill="#d6e3e1">
                     <font>
                        <Font size="24.0" />
                     </font>
                     <padding>
                        <Insets right="5.0" />
                     </padding>
                  </Label>
                  <Label fx:id="email" alignment="CENTER" prefHeight="50.0" prefWidth="1280.0" text="user email" textFill="#d6e3e1">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
               </children>
            </VBox>
         </top>
      </BorderPane>
      <Button fx:id="cancelButton" layoutX="1156.0" mnemonicParsing="false" onAction="#handleCancelButton" styleClass="button-controlP" text="X" textFill="#003b3b">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
      <Button fx:id="minimizeButton" layoutX="1132.0" mnemonicParsing="false" onAction="#handleMinimizeButton" styleClass="button-controlP" text="-" textFill="#003b3b">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
   </children>
